@article{ ISI:000343869100009,
Author = {Viricel, Amelia and Rosel, Patricia E.},
Title = {{Hierarchical population structure and habitat differences in a highly
   mobile marine species: the Atlantic spotted dolphin}},
Journal = {{MOLECULAR ECOLOGY}},
