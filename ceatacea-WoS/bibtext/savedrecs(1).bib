@article{ ISI:000331690600013,
Author = {Olavarria, C. and Baker, C. S. and Tezanos-Pinto, G.},
Title = {{Low mtDNA genetic diversity among killer whales around New Zealand}},
Journal = {{NEW ZEALAND JOURNAL OF MARINE AND FRESHWATER RESEARCH}},
Year = {{2014}},
Volume = {{48}},
Number = {{1}},
Pages = {{147-153}},
Month = {{JAN 2}},
Abstract = {{We report here the genetic diversity of killer whales around New Zealand
   and compare samples collected in this region (n = 11) with larger
   geographic databases of mtDNA control region sequences to investigate
   the relationship of the New Zealand killer whales with more distant
   populations/ecotypes. Eight variable sites defined four haplotypes,
   revealing a low mtDNA genetic diversity when compared with other
   cetacean species and to that observed worldwide for killer whales. The
   geographic distribution and segregation of haplotypes within New Zealand
   suggested that this population could be geographically structured. Only
   one of the New Zealand haplotypes matched with those from other distant
   regions (the Eastern North Atlantic and Western South Atlantic
   populations).}},
