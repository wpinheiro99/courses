@article{ ISI:000333803200026,
Author = {Racicot, Rachel A. and Demere, Thomas A. and Beatty, Brian L. and
   Boessenecker, Robert W.},
Title = {{Unique Feeding Morphology in a New Prognathous Extinct Porpoise from the
   Pliocene of California}},
