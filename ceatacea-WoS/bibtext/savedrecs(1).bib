@article{ ISI:000337968200003,
Author = {Alessi, Jessica and Aissi, Mehdi and Fiori, Cristina},
Title = {{Photo-identification of sperm whales in the north-western Mediterranean
   Sea: an assessment of natural markings}},
Journal = {{AQUATIC CONSERVATION-MARINE AND FRESHWATER ECOSYSTEMS}},
