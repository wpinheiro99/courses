@article{ ISI:000353217100015,
Author = {Gill, Peter C. and Pirzl, Rebecca and Morrice, Margie G. and Lawton,
   Kieran},
Title = {{Cetacean Diversity of the Continental Shelf and Slope off Southern
   Australia}},
Journal = {{JOURNAL OF WILDLIFE MANAGEMENT}},
