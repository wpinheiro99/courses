Title = {{Linking the occurrence of cutaneous opportunistic fungal invaders with
   elemental concentrations in false killer whale (Pseudorca crassidens)
   skin}},
Journal = {{ENVIRONMENTAL MICROBIOLOGY REPORTS}},
