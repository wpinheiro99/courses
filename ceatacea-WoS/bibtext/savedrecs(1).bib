@article{ ISI:000342835800012,
Author = {Jefferson, Thomas A. and Rosenbaum, Howard C.},
Title = {{Taxonomic revision of the humpback dolphins (Sousa spp.), and
   description of a new species from Australia}},
Journal = {{MARINE MAMMAL SCIENCE}},
