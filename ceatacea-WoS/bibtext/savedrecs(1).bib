@article{ ISI:000337968200008,
Author = {Mussi, Barbara and Miragliuolo, Angelo and Zucchini, Antonio and Pace,
   Daniela S.},
Title = {{Occurrence and spatio-temporal distribution of sperm whale (Physeter
   macrocephalus) in the submarine canyon of Cuma (Tyrrhenian Sea, Italy)}},
Journal = {{AQUATIC CONSERVATION-MARINE AND FRESHWATER ECOSYSTEMS}},
Year = {{2014}},
Volume = {{24}},
Number = {{1}},
Pages = {{59-70}},
Month = {{JUL}},
Abstract = {{Boat surveys aimed at studying sperm whales in the Tyrrhenian Sea were
   conducted between 2002 and 2011. During 768 daily surveys, a total
   effort of 32602km was achieved within an area of 8800km2 resulting in 92
   encounters with 229 sperm whale individuals. Average encounter rates of
   sperm whales was 0.5 groups per 100km2, with a higher concentration in
   the vicinity of the submarine canyon of Cuma, confirming the importance
   for the species of this small hotspot in the Mediterranean Sea.
   Encounter rates increased with increasing distance from the coast. It is
   possible that the intense boat traffic and anthropogenic disturbance in
   the area may be moving animals away from the coast leading to habitat
   loss. The species-habitat relationship documented in this study has
   implications for conservation. Copyright (c) 2014 John Wiley \& Sons,
   Ltd.}},
