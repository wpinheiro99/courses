@article{ ISI:000342216600008,
Author = {Arcoverde, Danilo Leal and Emin-Lima, Renata and Costa, Alexandra
   Fernandes and Madeira Di Beneditto, Ana Paula and Siciliano, Salvatore
   and Sena, Leonardo and Moreno, Ignacio Benites and De Sousa E Silva,
   Jr., Jose},
Title = {{Evaluation of periotic-timpanic bone complex of Sotalia guianensis
   (Cetacea: Delphinidae) as tool in identification of geographic
   variations}},
Journal = {{JOURNAL OF THE MARINE BIOLOGICAL ASSOCIATION OF THE UNITED KINGDOM}},
Year = {{2014}},
Volume = {{94}},
Number = {{6}},
Pages = {{1127-1132}},
Month = {{SEP}},
Abstract = {{Morphometric characteristics of the periotic-timpanic bone complex in
   the middle ear of cetaceans, are effective characteristics in evaluating
   systematics. However, they have not been used for studies of geographic
   variation regarding dolphins of the genus Sotalia. This study aimed to
   compare the periotic-timpanic of Sotalia guianensis from four distinct
   locations, considered here as different operational taxonomic units,
   Amapa/Para (AM/PA), Maranhao/Piaui (MA/PI), Ceara (CE), and Rio de
   Janeiro (RJ), using 21 morphometric measurements. Multivariate analysis
   showed significant distinction mainly between the units of northern
   (AM/PA and MA/PI) and south-eastern (RJ) Brazilian coast. The timpanic
   bone showed variation, reaching larger sizes in the Brazilian south
   coast unit, corroborating current molecular data on the geographic
   variation of S. guianensis.}},
